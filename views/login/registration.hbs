<form action="/login/registration" method="POST" autocomplete="off">
    <div class="wrapper">
    <label>Username</label>
    <input type="text" name="username" placeholder="Username" value="{{list.username}}" required>
    <div class="text-danger">
            {{list.validationError}}
    </div>
    <label>Password</label>
    <input type="text" name="password" placeholder="Password" value="{{list.password}}" required>
    <label>Name</label>
    <input type="text" name="name" placeholder="Name" value="{{list.name}}" required>
    <label>Last name</label>
    <input type="text" name="lastName" placeholder="Last Name" value="{{list.lastName}}" required>
    <label>Address</label>
    <input type="text" name="address" placeholder="Address" value="{{list.address}}" required>
    <label>Phone number</label>
    <input type="tel" name="pNumber" placeholder="Phone number" value="{{list.pNumber}}" pattern="[0-9]{9}" required>
    <button type="submit">Submit</button>
    </div>
</form>
</form>